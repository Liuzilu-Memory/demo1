        <xml><![CDATA[<g:ui_macro name="step_navigation_macro">
  <table>
    <tr>
      <td><button onclick="setStep('step1')">Step1</button></td>
      <td><button onclick="setStep('step2')">Step2</button></td>
      <td><button onclick="setStep('step3')">Step3</button></td>
    </tr>
  </table>
  <script>
    function setStep(step) {
      try {
        g_form.setValue('current_step', step);
        g_form.refresh();  // 触发 onLoad
      } catch(e) {
        console.log("Error setting step: " + e);
      }
    }
  </script>
</g:ui_macro>]]></xml>
